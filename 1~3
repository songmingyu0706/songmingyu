        "y1 = int(input(\"y1: \"))\n",
        "x2 = int(input(\"x2: \"))\n",
        "y2 = int(input(\"y2: \"))\n",
        "dist = ((x1 - x2) ** 2 + (y1 - y2) ** 2) ** 0.5\n",
        "t.up()\n",
        "t.goto(x1, y1)\n",
        "t.down()\n",
        "t.goto(x2, y2)\n",
